{
  "name": "kuborgh/csv-bundle",
  "type": "symfony-bundle",
  "description": "Converting an array to CSV and vice versa.",
  "keywords": [
    "library",
    "csv",
    "array",
    "rfc4180",
    "excel"
  ],
  "homepage": "https://github.com/kuborgh/csv-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Hannes Giesenow",
      "email": "hgiesenow@kuborgh.de"
    }
  ],
  "require": {
    "symfony/framework-bundle": "~2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "symfony/property-access": "~2.3"
  },
  "autoload": {
    "psr-0": {
      "Kuborgh\\CsvBundle": ""
    }
  },
  "config" : {
    "bin-dir" : "bin"
  },
  "target-dir": "Kuborgh/CsvBundle"
}
